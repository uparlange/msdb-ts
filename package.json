{
  "name": "msdb-ts",
  "version": "5.2.1",
  "author": "Ugo Parlange",
  "description": "Mame Smart Database",
  "homepage": "https://msdb.lapli.fr",
  "synopsis": "MAME 'Multiple Arcade Machine Emulator' est un émulateur de jeux d'arcade. La liste des jeux est telle qu'il est considéré comme une sorte de grande bibliothèque ou de 'mémoire' du jeu d'arcade. Pour vérifier que votre jeu préféré est disponible, allez faire une recherche sur MSDB !",
  "favicon": "assets/favicons/apple-touch-icon.png",
  "repository": {
    "type": "git",
    "url": "git://github.com/uparlange/msdb-ts.git"
  },
  "bugs": {
    "url": "https://github.com/uparlange/msdb-ts/issues"
  },
  "main": "http://localhost",
  "node-remote": "http://localhost",
  "window": {
    "title": "Mame Smart Database",
    "min_width": 500,
    "width": 500,
    "min_height": 700,
    "height": 700
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist-ng ./dist-nwjs",
    "debug": "ng serve --port=80",
    "web:debug": "npm run debug",
    "web:build-prod": "ng build --prod --output-path ./dist-ng",
    "postweb:build-prod": "node ./scripts/postbuild.js",
    "preweb:debug-prod": "npm run clean",
    "web:debug-prod": "npm run web:build-prod",
    "postweb:debug-prod": "node server.js",
    "desktop:debug": "concurrently \"npm run debug\" \"wait-on http://localhost && nw .\"",
    "desktop:build-prod": "ng build --prod --serviceWorker=false --output-path ./dist-ng",
    "predesktop:release-win64": "npm run clean",
    "desktop:release-win64": "npm run desktop:build-prod",
    "postdesktop:release-win64": "build --tasks win-x64 --mirror https://dl.nwjs.io/ ."
  },
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.7",
    "@angular/cdk": "^9.0.0-rc.6",
    "@angular/common": "^9.0.0-rc.7",
    "@angular/compiler": "^9.0.0-rc.7",
    "@angular/core": "^9.0.0-rc.7",
    "@angular/forms": "^9.0.0-rc.7",
    "@angular/material": "^9.0.0-rc.6",
    "@angular/platform-browser": "^9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.7",
    "@angular/pwa": "^0.803.21",
    "@angular/router": "^9.0.0-rc.7",
    "@angular/service-worker": "^9.0.0-rc.7",
    "@mdi/angular-material": "^4.7.95",
    "blazy": "^1.8.2",
    "body-parser": "^1.19.0",
    "chokidar": "^3.3.1",
    "dexie": "^2.0.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "masonry-layout": "^4.2.2",
    "nsis-compat-updater": "^1.2.0",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.5.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.7",
    "@angular/cli": "^9.0.0-rc.7",
    "@angular/compiler-cli": "^9.0.0-rc.7",
    "@angular/language-service": "^9.0.0-rc.7",
    "@types/blazy": "^1.5.31",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/masonry-layout": "^4.2.1",
    "@types/node": "^12.12.21",
    "@types/photoswipe": "^4.0.29",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^5.2.1",
    "concurrently": "^5.0.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nw": "^0.43.2-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  },
  "chromium-args": "--enable-node-worker",
  "build": {
    "nwVersion": "v0.43.2",
    "nwFlavor": "normal",
    "output": "dist-nwjs",
    "targets": [
      "zip",
      "nsis7z"
    ],
    "files": [
      "**/*"
    ],
    "excludes": [
      ".theia/**/*",
      "e2e/**/*",
      "release/**/*",
      "scripts/**/*",
      "src/**/*",
      ".editorconfig",
      ".gitignore",
      "angular.json",
      "browserslist",
      "karma.conf.js",
      "ngsw-config.json",
      "package-lock.json",
      "README.md",
      "server.js",
      "tsconfig.app.json",
      "tsconfig.json",
      "tsconfig.spec.json",
      "tslint.json"
    ],
    "strippedProperties": [
      "ManifestComments",
      "node-remote",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "dist-ng/index.html",
      "node-remote": ""
    },
    "win": {
      "icon": "src/assets/favicons/favicon.ico"
    },
    "mac": {
      "icon": "assets/icon.icns"
    },
    "nsis": {
      "icon": "src/assets/favicons/favicon.ico",
      "unIcon": "src/assets/favicons/favicon.ico",
      "languages": [
        "English"
      ],
      "diffUpdaters": false,
      "hashCalculation": true
    }
  }
}
