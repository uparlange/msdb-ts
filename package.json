{
  "name": "msdb-ts",
  "version": "5.6.0",
  "author": "Ugo Parlange",
  "description": "Mame Smart Database",
  "homepage": "https://msdb.lapli.fr",
  "synopsis": "MAME 'Multiple Arcade Machine Emulator' est un émulateur de jeux d'arcade. La liste des jeux est telle qu'il est considéré comme une sorte de grande bibliothèque ou de 'mémoire' du jeu d'arcade. Pour vérifier que votre jeu préféré est disponible, allez faire une recherche sur MSDB !",
  "favicon": "assets/favicons/apple-touch-icon.png",
  "repository": {
    "type": "git",
    "url": "git://github.com/uparlange/msdb-ts.git"
  },
  "bugs": {
    "url": "https://github.com/uparlange/msdb-ts/issues"
  },
  "main": "http://localhost",
  "node-remote": "http://localhost",
  "window": {
    "title": "Mame Smart Database",
    "min_width": 500,
    "width": 500,
    "min_height": 700,
    "height": 700
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist-ng/* ./dist-nwjs/*",
    "debug": "ng serve --port=80",
    "web:debug": "npm run debug",
    "preweb:build-prod": "npm run clean",
    "web:build-prod": "ng build --configuration production --output-path ./dist-ng",
    "postweb:build-prod": "node ./scripts/postbuild.js",
    "web:debug-prod": "npm run web:build-prod",
    "postweb:debug-prod": "node server.js",
    "desktop:debug": "concurrently \"npm run debug\" \"wait-on http://localhost && nw .\"",
    "predesktop:build-prod": "npm run clean",
    "desktop:build-prod": "ng build --configuration production --serviceWorker=false --output-path ./dist-ng",
    "postdesktop:build-prod": "node ./scripts/postbuild.js",
    "desktop:release-win64": "npm run desktop:build-prod",
    "postdesktop:release-win64": "build --tasks win-x64 --mirror https://dl.nwjs.io/ ."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.2",
    "express": "^4.17.1",
    "nsis-compat-updater": "^1.2.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.3",
    "@angular/animations": "^12.2.3",
    "@angular/cdk": "^12.2.3",
    "@angular/cli": "^12.2.3",
    "@angular/common": "^12.2.3",
    "@angular/compiler": "^12.2.3",
    "@angular/compiler-cli": "^12.2.3",
    "@angular/core": "^12.2.3",
    "@angular/forms": "^12.2.3",
    "@angular/language-service": "^12.2.3",
    "@angular/material": "^12.2.3",
    "@angular/platform-browser": "^12.2.3",
    "@angular/platform-browser-dynamic": "^12.2.3",
    "@angular/pwa": "^12.2.3",
    "@angular/router": "^12.2.3",
    "@angular/service-worker": "^12.2.3",
    "@mdi/angular-material": "^5.9.55",
    "@types/blazy": "^1.5.33",
    "@types/jasmine": "^3.8.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/masonry-layout": "^4.2.4",
    "@types/node": "^16.3.3",
    "@types/photoswipe": "^4.1.2",
    "blazy": "^1.8.2",
    "chart.js": "^3.4.1",
    "chartjs-plugin-datalabels": "^2.0.0",
    "codelyzer": "^6.0.1",
    "concurrently": "^6.2.0",
    "dexie": "^3.0.3",
    "fs-extra": "^10.0.0",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "masonry-layout": "^4.2.2",
    "nw": "0.55.0-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "photoswipe": "^4.1.3",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io-client": "^4.1.3",
    "svg-country-flags": "^1.2.10",
    "svgstore": "^3.0.0-2",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "update": "^0.7.4",
    "wait-on": "^6.0.0",
    "zone.js": "~0.11.4"
  },
  "chromium-args": "--enable-node-worker",
  "build": {
    "nwVersion": "v0.55.0",
    "nwFlavor": "normal",
    "output": "dist-nwjs",
    "targets": [
      "zip",
      "nsis7z"
    ],
    "files": [
      "**/*"
    ],
    "excludes": [
      ".theia/**/*",
      "e2e/**/*",
      "release/**/*",
      "scripts/**/*",
      "src/**/*",
      ".editorconfig",
      ".gitignore",
      "angular.json",
      "browserslist",
      "karma.conf.js",
      "ngsw-config.json",
      "package-lock.json",
      "README.md",
      "server.js",
      "tsconfig.app.json",
      "tsconfig.json",
      "tsconfig.spec.json",
      "tslint.json"
    ],
    "strippedProperties": [
      "ManifestComments",
      "node-remote",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "dist-ng/index.html",
      "node-remote": ""
    },
    "win": {
      "icon": "src/assets/favicons/favicon.ico"
    },
    "mac": {
      "icon": "assets/icon.icns"
    },
    "nsis": {
      "icon": "src/assets/favicons/favicon.ico",
      "unIcon": "src/assets/favicons/favicon.ico",
      "languages": [
        "English"
      ],
      "diffUpdaters": false,
      "hashCalculation": true
    }
  }
}
