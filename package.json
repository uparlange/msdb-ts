{
  "name": "msdb-ts",
  "version": "5.1.8",
  "author": "Ugo Parlange",
  "description": "Mame Smart Database",
  "homepage": "https://msdb.lapli.fr",
  "synopsis": "MAME 'Multiple Arcade Machine Emulator' est un émulateur de jeux d'arcade. La liste des jeux est telle qu'il est considéré comme une sorte de grande bibliothèque ou de 'mémoire' du jeu d'arcade. Pour vérifier que votre jeu préféré est disponible, allez faire une recherche sur MSDB !",
  "favicon": "assets/favicons/apple-touch-icon.png",
  "repository": {
    "type": "git",
    "url": "git://github.com/uparlange/msdb-ts.git"
  },
  "bugs": {
    "url": "https://github.com/uparlange/msdb-ts/issues"
  },
  "main": "http://localhost",
  "node-remote": "http://localhost",
  "ws-port": "56789",
  "window": {
    "title": "Mame Smart Database",
    "min_width": 500,
    "width": 500,
    "min_height": 700,
    "height": 700,
    "icon": "assets/favicons/apple-touch-icon.png"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=80",
    "build": "ng build",
    "postbuild": "node ./scripts/postbuild.js",
    "build-prod": "ng build --prod",
    "postbuild-prod": "node ./scripts/postbuild.js",
    "build-prod-nw": "ng build --prod --serviceWorker=false",
    "postbuild-prod-nw": "node ./scripts/postbuild.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./scripts/postinstall.js",
    "web-debug": "npm run start",
    "web-prod": "node server.js",
    "preweb-prod": "npm run build-prod",
    "desktop-debug": "concurrently \"npm run web-debug\" \"wait-on http://localhost && nw .\"",
    "desktop-prod": "nw ./dist/msdb-ts/",
    "predesktop-prod": "npm run build-prod-nw",
    "release-nw": "npm run build-prod-nw",
    "postrelease-nw": "node ./scripts/postreleasenw.js"
  },
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.3",
    "@angular/cdk": "^9.0.0-rc.4",
    "@angular/common": "^9.0.0-rc.3",
    "@angular/compiler": "^9.0.0-rc.3",
    "@angular/core": "^9.0.0-rc.3",
    "@angular/forms": "^9.0.0-rc.3",
    "@angular/material": "^9.0.0-rc.4",
    "@angular/platform-browser": "^9.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.3",
    "@angular/pwa": "^0.803.19",
    "@angular/router": "^9.0.0-rc.3",
    "@angular/service-worker": "^9.0.0-rc.3",
    "@mdi/angular-material": "^4.6.95",
    "blazy": "^1.8.2",
    "body-parser": "^1.19.0",
    "chokidar": "^3.3.0",
    "dexie": "^2.0.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "masonry-layout": "^4.2.2",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.5.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.3",
    "@angular/cli": "^9.0.0-rc.3",
    "@angular/compiler-cli": "^9.0.0-rc.3",
    "@angular/language-service": "^9.0.0-rc.3",
    "@types/blazy": "^1.5.31",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/masonry-layout": "^4.2.1",
    "@types/node": "^12.12.12",
    "@types/photoswipe": "^4.0.29",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^5.2.0",
    "concurrently": "^5.0.0",
    "fs-extra": "^8.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nw": "0.42.5-sdk",
    "protractor": "~5.4.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0",
    "zip-a-folder": "0.0.9"
  }
}
