    "@angular/language-service": "~8.0.3",
    "@types/blazy": "^1.5.31",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/masonry-layout": "^4.2.1",
    "@types/node": "~8.9.4",
    "@types/photoswipe": "^4.0.29",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^5.0.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nw": "^0.39.2-sdk",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
