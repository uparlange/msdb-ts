{
  "name": "msdb-ts",
  "version": "5.0.2",
  "author": "Ugo Parlange",
  "description": "Mame Smart Database",
  "homepage": "https://msdb.lapli.fr",
  "synopsis": "MAME 'Multiple Arcade Machine Emulator' est un émulateur de jeux d'arcade. La liste des jeux est telle qu'il est considéré comme une sorte de grande bibliothèque ou de 'mémoire' du jeu d'arcade. Pour vérifier que votre jeu préféré est disponible, allez faire une recherche sur MSDB !",
  "favicon": "images/favicon/apple-touch-icon.png",
  "repository": {
    "type": "git",
    "url": "git://github.com/uparlange/msdb-ts.git"
  },
  "bugs": {
    "url": "https://github.com/uparlange/msdb-ts/issues"
  },
  "main": "index.html",
  "window": {
    "title": "Mame Smart Database",
    "min_width": 500,
    "width": 500,
    "min_height": 700,
    "height": 700,
    "icon": "assets/favicon/apple-touch-icon.png"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "cpx ./package.json ./dist/msdb-ts/",
    "build-prod": "ng build --prod",
    "postbuild-prod": "cpx ./package.json ./dist/msdb-ts/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./postinstall.js",
    "web-debug": "ng serve --port=80",
    "web-prod": "node server",
    "preweb-prod": "npm run build-prod",
    "desktop-debug": "nw ./dist/msdb-ts/",
    "predesktop-debug": "npm run build",
    "desktop-prod": "nw ./dist/msdb-ts/",
    "predesktop-prod": "npm run build-prod"
  },
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/forms": "~8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/pwa": "^0.800.6",
    "@angular/router": "~8.0.3",
    "@angular/service-worker": "~8.0.3",
    "@mdi/angular-material": "^3.6.95",
    "blazy": "^1.8.2",
    "body-parser": "^1.19.0",
    "chokidar": "^3.0.1",
    "dexie": "^2.0.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "masonry-layout": "^4.2.2",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.5.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
